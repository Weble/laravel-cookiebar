{
    "name": "weble/laravel-cookiebar",
    "description": "Laravel Cookies Consent Management for the new EU Cookie LAW",
    "keywords": [
        "weble",
        "cookie-consent",
        "cookiebar",
        "cookie",
        "comply",
        "law",
        "eu"
    ],
    "homepage": "https://github.com/Weble/laravel-cookiebar",
    "license": "MIT",
    "authors": [
        {
            "name": "Enea Pavanello",
            "email": "enea@weble.it",
            "homepage": "https://weble.it",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^7.0|^8.0|^9.0",
        "illuminate/view": "^7.0|^8.0|^9.0",
        "illuminate/cookie": "^7.0|^8.0|^9.0",
        "spatie/laravel-package-tools": "^1.9"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9",
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Weble\\Cookiebar\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Weble\\Cookiebar\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Weble\\Cookiebar\\CookiebarServiceProvider"
            ]
        }
    },
    "prefer-stable": true
}
